    "import os\n",
    "import glob\n",
    "\n",
    "with open(\"lebels\")\n",
    "\n",
    "file_names = []\n",
    "path = os.path.join(os.getcwd(), \"*.png\")\n",
    "for f in glob.glob(path):\n",
    "    if not \"mask\" in f:\n",
    "        #print(f)\n",
    "        file_names.append(\"ML \"+f.split()[-1])\n",
    "\n",
    "print(len(file_names))\n",
    "\n",
    "file_names[0]\n"
   ]
